{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DatabaseSettings": {
    "Ip": "${DB_HOST}",
    "Port": 5432,
    "User": "${DB_USER}",
    "Password": "${DB_PASSWORD}",
    "Database": "${DB_NAME}",
    "Pooling": true,
    "MaximumPoolSize": 5,
    "MinimumPoolSize": 0,
    "Timeout": 15
  },
  "APIService": {
    "BaseUrlAppointment": "${BASE_URL_APPOINTMENT}",
    "BaseUrlServiceDetail": "${BASE_URL_SERVICEDETAIL}"
  },
  "Swagger": {
    "Enabled": true
  },
  "AllowedHosts": "*"
}
